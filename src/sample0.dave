tbl simpleLinearRegression(tbl sample, str ind, str dep) {
  int sizeX = sample.col_length;
  float avgX = 0.0;
  float avgY = 0.0;
  float sumX = 0.0;
  float sumY = 0.0;
  float cov = 0.0;
  float var = 0.0;
  int i = 0;
  fld x = access(sample,ind);
  fld y = access(sample,dep);
  for (i = 0; i < sizeX; i++) {
    sumX = sumX + x[i];
    sumY = sumY + y[i];
  }
  avgX = sumX/sizeX;
  avgY = sumY/sizeY;
  for (i = 0; i < sizeX; i++) {
    cov = cov + ((x[i]-avgX)*(y[i]-avgY));
    var = var + ((x[i]-avgX)*(x[i]-avgX));
  }
  float beta = cov/var;
  float alpha = avgY - beta * avgX;
  float[sizeX] f;
  for (i = 0; i < sizeX; i++) {
    f[i] = alpha + beta * x[i];
  }
  fld f3 = new fld(f,"est");
  float ssres = 0.0;
  float sstot = 0.0;
  for (i = 0; i < sizeX; i++) {
    ssres = ssres + ((y[i]-f[i])*(y[i]-f[i]));
    sstot = sstot + ((y[i]-avgY)*(y[i]-avgY));
  }
  float rsquare = 1 - ssres/sstot;
  print("The beta value is");
  print(beta);
  print("The alpha value is");
  print(alpha);
  print("The R-Square value is (%)");
  print(rsquare*100);
  tbl newsample = append(sample,f3);
  return newsample;
}




