int main() {
	str a = "hello world!";
	int c;
	// this is a single line comment
	for (c = 0; c < 10; c++) {
		print(a);
	}
	bool is_continue = true;

	int d = 1;
	int e = 0;
	while (is_continue) {
		if (d > 10)
			break;
		print("in while loop");
		e += d;
		d += 1;
	}
	print("printing sum of 1 to 10:");
	print(e);

	str[4] f;

	f[0] = "hello!";
	print(f[0]);
	return 0;
}
